StateVariables          = [y1, y2];
ControlVariables        = [u1];
InitialConstraints      = [y1 - 1, y2];
TerminalPenalty         = y2;
DifferentialEquations   = [u1, y1*y1 + u1*u1];
StateEstimate           = [1 + t, t*(t-1)];
